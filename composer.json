{
  "name": "rugaard/dmi",
  "type": "library",
  "description": "API for the Danish Meteorological Institute (DMI)",
  "keywords": [
    "dmi",
    "danish meteorological institute",
    "dmi api",
    "danish meteorological institute api",
    "api",
    "weather",
    "forecast",
    "weather forecast",
    "morten",
    "rugaard",
    "morten rugaard"
  ],
  "authors": [
    {
      "name": "Morten Rugaard",
      "email": "morten@rugaard.me",
      "homepage": "https://github.com/rugaard",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/rugaard",
  "support": {
    "issues": "https://github.com/rugaard/dmi/issues",
    "source": "https://github.com/rugaard/dmi"
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "tightenco/collect": "^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.0",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Rugaard\\DMI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rugaard\\DMI\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rugaard\\DMI\\Providers\\Laravel\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --colors=always"
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
